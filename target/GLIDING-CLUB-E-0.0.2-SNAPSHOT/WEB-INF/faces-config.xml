<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<display-name>flightsList.xhtml</display-name>
		<from-view-id>/flightsList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>adminForm</from-outcome>
			<to-view-id>/administration/adminForm.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>bookingSuccess</from-outcome>
			<to-view-id>/showBookingResult.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>bookingError</from-outcome>
			<to-view-id>/flightsList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>siteDetail</from-outcome>
			<to-view-id>/siteDetail.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>siteDetail.xhtml</display-name>
		<from-view-id>/siteDetail.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>adminForm</from-outcome>
			<to-view-id>/administration/adminForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pilotForm.xhtml</display-name>
		<from-view-id>/administration/pilotForm.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>adminForm</from-outcome>
			<to-view-id>/administration/adminForm.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>siteForm.xhtml</display-name>
		<from-view-id>/administration/siteForm.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>adminForm</from-outcome>
			<to-view-id>/administration/adminForm.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>adminForm.xhtml</display-name>
		<from-view-id>/administration/adminForm.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>adminForm</from-outcome>
			<to-view-id>/administration/adminForm.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
